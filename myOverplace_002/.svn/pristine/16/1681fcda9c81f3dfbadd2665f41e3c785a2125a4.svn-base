angular.module("starter.filter",[]).filter("capitalize",function(){return function(t){return angular.isString(t)&&t.length>0?t[0].toUpperCase()+t.substr(1).toLowerCase():t}}).filter("stripUnderscore",function(){return function(t){return angular.isString(t)&&t.length>0?t.split("_").join(" "):t}}).filter("keys",function(){return function(t){return t&&"object"==typeof t?Object.keys(t):[]}});